{
  "expo": {
    "name": "infinitLMS",
    "scheme": "infinitlms",
    "slug": "infinitLMS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/infinit-lms-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/infinit-lms-splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#313131"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/infinit-lms-adaptive-icon.png",
        "backgroundColor": "#313131"
      }
    },
    "backgroundColor": "#313131",
    "androidStatusBar": {
      "backgroundColor": "#fff0",
      "translucent": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}